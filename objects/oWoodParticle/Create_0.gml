/// @description Timer Set Up

mode = irandom_range(0,1); // will determine if moving left or right
colorType = irandom_range(0,1); // if 1, change to sWoodParticle2

time = 45; // time spent alive

direction = irandom_range(10,50); // starting angle of movement




spin =  irandom_range(7,15); // speed of spin
speed = irandom_range(7,10); // movement speed of object
fallSpeed = speed * 1.2;

changeTo = -1 * irandom_range(55,85); // angle it will change to simulate gravity for right pieces



image_alpha = 1;




